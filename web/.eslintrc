{
  "env": {
    "browser": true,
    "mocha": true,
    "node": false,
    "amd": true
  },
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base"
  ],
  "ecmaFeatures": {
    "modules": false,
    "jsx": true
  },
  "rules": {
    "indent": [
      2,
      2
    ],
    "new-cap": 0,
    "class-methods-use-this": 0,
    "comma-dangle": 0,
    "func-names": 0,
    "import/extensions": [1, "never"],
    "import/no-unresolved": 0,
    "import/no-webpack-loader-syntax": 0,
    "no-plusplus": 0,
    "no-var": 1,
    "space-before-function-paren": [1, "always"],
    "vars-on-top": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "max-len": 0,
    "react/jsx-uses-vars": 1,
    "object-shorthand": [0, "always"],
    "strict": 0
  },
  "plugins": [
    "react"
  ]
}
